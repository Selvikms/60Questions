include <stdio.h>
void main()
{
    int i,j,n,k=48;
    printf("Enter the number:");
    scanf("%d",n);
    for(i=0;i<n;i++)
    {
        for(j=0;j<n-i;j++)
        {
        printf(" ");
        }
        for(j=0;j<i;j++)
        {
            printf("%c",k);
        }
        k++;
        printf("\n");
        
    }
}



1
     22
    333
   4444
  55555
 666666
