include<stdio.h>
void main()
{
    int i,j,n=4;
    // printf("Enter the number:");
    // scanf("%d",&n);
    
    for(i=1;i<=3;i++)
    {
        for(j=1;j<=3;j++)
        {
            printf("%d ",n);
            n=n+4;
        }
        printf("\n");
    }
}
    
             
             
             
             4 8 12 
16 20 24 
28 32 36 
